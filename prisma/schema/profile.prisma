model profile {
  id            String    @id @default(cuid())
  fullname      String?
  gender        String?
  phoneNumber   String?
  address       String?
  country       String?
  city          String?
  maritalStatus String?
  dateOfBirth   DateTime?
  firstName     String?
  lastName      String?
  middleName    String?
  profileUrl    String?
  zip           String?
  unit          String?
  state         String?
  timeZone      String?
  taxPayerId    String?
  taxType       String?
  title         String?

  // relations fields 
  users     users?
}


enum RiskLevel {
  EXCELLENT  // 850-950
  GOOD       // 700-849
  FAIR       // 550-699
  POOR       // 400-549
  CRITICAL   // 0-399
}